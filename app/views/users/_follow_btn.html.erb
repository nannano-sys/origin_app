<% if current_user != user %>
  <% if user_signed_in?%>
        <% if user.followed_by?(current_user) %>
            <p><%=link_to "フォロー解除", user_relationship_path(user.id), method: :delete %></p>
        <% else %>
            <p><%=link_to "フォローする", user_relationships_path(user.id), method: :post %></p>
        <% end %>
    <%end%>
  <% end %>